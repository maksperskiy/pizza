services:
  pizza:
    build:
      context: D:\универ\5 курс\Данные в ИИТ\Pizza\back-end\pizza
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: pizza
      target: base
    container_name: pizza_1
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: pizza:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/pizza.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8120
      target: 80
    - published: 8121
      target: 443
    tty: true
    volumes:
    - D:\универ\5 курс\Данные в ИИТ\Pizza\back-end\pizza:/app:rw
    - C:\Users\computar\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\computar\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\computar\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\computar\.nuget\packages:/root/.nuget/packages:ro
    - D:\универ\5 курс\Данные в ИИТ\Pizza\back-end\pizza:/src:rw
version: '3.4'